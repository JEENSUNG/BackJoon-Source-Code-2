import java.util.*;
public class Main{
    public static void main(String[] args){
        Scanner scan = new Scanner(System.in);
        int n = scan.nextInt();
        int m = scan.nextInt();
        int k = scan.nextInt();

        int day = (k - m) / (n - m);
        if((k-m) % (n - m) != 0)
            day++;
        System.out.println(day);
    }
}
