import java.util.*;
public class Main{
    public static void main(String[] args){
        Scanner scan = new Scanner(System.in);
        ArrayList<Integer> list = new ArrayList<>();
        for(int i = 0; i < 3; i++){
            int a = scan.nextInt();
            list.add(a);
        }
        Collections.sort(list);
        System.out.println(list.get(list.size() - 2));
    }
}
